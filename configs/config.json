{
  "name": "GAN",
  "n_gpu": 1,

  "arch": {
    "type": "SNGAN",
    "args": {
      "latent_dim": 100
    }
  },
  "data_loader": {
    "type": "CelebA64DataLoader",
    "args": {
      "data_dir": "data/",
      "batch_size": 32,
      "shuffle": true,
      "num_workers": 2
    }
  },

  "optimizer_G": {
    "type": "Adam",
    "args": {
      "lr": 0.001,
      "weight_decay": 0,
      "amsgrad": true

    }
  },
  "optimizer_D": {
    "type": "Adam",
    "args": {
      "lr": 0.001,
      "weight_decay": 0,
      "amsgrad": true

    }
  },
  "loss": "adversarial_loss",
  "lr_scheduler_G": {
    "type": "StepLR",
    "args": {
      "step_size": 50,
      "gamma": 0.1
    }
  },
  "lr_scheduler_D": {
    "type": "StepLR",
    "args": {
      "step_size": 50,
      "gamma": 0.1
    }
  },
  "trainer": {
    "type": "WGANGPTrainer",

    "epochs": 100,

    "save_dir": "saved/",
    "save_period": 1,
    "verbosity": 2,

    "visual_tool": "torch.utils.tensorboard",
    "__comment_1.1": "torch.utils.tensorboard",
    "__comment_1.2": "tensorboardX",
    "__comment_1.3": "wandb",
    "__comment_1.4": "None",
    "api_key_file": "./init/wandb-api-key-file",
    "project": "gan-ada",
    "entity": "gan-augment-project",
    "name": "None",
    "__comment_2.1": "Set name for one running"
  },
  "eval": {
    "save_dir": "saved/generated",
    "n_sample": 1000,
    "batch_size": 8
  },
  "augment": {
    "type": "Ada",
    "args": {
      "xflip": 1,
      "rotate90": 1,
      "xint": 1,
      "scale":1,
      "aniso":1,
      "xfrac":1,
      "lumaflip":1,
      "hue":1,
      "saturation": 1,
      "brightness": 1,
      "contrast": 1,
      "cutout": 1
    }
  }
}
